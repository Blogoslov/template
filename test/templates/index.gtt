[% TEMPLATE Test(header string, users []User) USE WRAPPER page(header) %]
    <h1>[% header | HTML %]</h1>
    <hr>
    [% FOR user IN users %]
        <p>
            [% PROCESS UserName(user) %]:
            [% user.Age %] ([% IF user.IsMan %]Man[% ELSE %]Woman[% END %])
        </p>
    [% END %]
[% END %]

[% TEMPLATE UserName(user User) %]
    [% user.Name | HTML +%] [% user.Lastname | HTML %]
[% END %]