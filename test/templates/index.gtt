[% WRAPPER page(caption string) %]
    <!DOCTYPE html>
    <html>
    <head>
        <title>[% caption | HTML %]</title>
    </head>
    <body>
        [% <CONTENT> %]
    </<body>
    </html>
[% END %]

[% TEMPLATE Test(header string, users []User) USE WRAPPER page(header) %]
    <h1>[% header | HTML %]</h1>
    <hr>
    [% FOR user IN users %]
        <p>
            [% user.Name | HTML %] [% user.Lastname | HTML %]:
            [% user.Age %] ([% IF user.IsMan %]Man[% ELSE %]Woman[% END %])
        </p>
    [% END %]
[% END %]